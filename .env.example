# Example Environment Variables
# Copy this file to .env and fill in your values
# Contact the development team for actual API endpoints and configuration values

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-domain.com/api
REACT_APP_ENVIRONMENT=production

# App Configuration
REACT_APP_NAME="AfricArtz Admin Dashboard"
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_DEBUG_MODE=false

# Notification Configuration
REACT_APP_NOTIFICATION_CHECK_INTERVAL=30000
REACT_APP_MAX_NOTIFICATIONS_DISPLAY=50

# API Configuration
REACT_APP_API_TIMEOUT=10000
REACT_APP_RETRY_ATTEMPTS=3

# Optional: Push Notification Keys (for future implementation)
# REACT_APP_VAPID_PUBLIC_KEY=your_vapid_public_key_here
# REACT_APP_FCM_SENDER_ID=your_fcm_sender_id_here