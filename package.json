		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"env:check": "node -e \"console.log('Environment variables:'); console.log('REACT_APP_API_BASE_URL:', process.env.REACT_APP_API_BASE_URL); console.log('NODE_ENV:', process.env.NODE_ENV);\""
	},
	"eslintConfig": {
		"extends": [
