# Development Environment Variables
REACT_APP_API_BASE_URL=http://localhost:8080/api
REACT_APP_ENVIRONMENT=development

# App Configuration
REACT_APP_NAME="AfricArtz Admin Dashboard (Dev)"
REACT_APP_VERSION=1.0.0-dev

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false
REACT_APP_DEBUG_MODE=true

# Notification Configuration
REACT_APP_NOTIFICATION_CHECK_INTERVAL=5000
REACT_APP_MAX_NOTIFICATIONS_DISPLAY=10

# API Configuration
REACT_APP_API_TIMEOUT=15000
REACT_APP_RETRY_ATTEMPTS=1

# Development specific
REACT_APP_MOCK_NOTIFICATIONS=true
REACT_APP_CONSOLE_LOGS=true
